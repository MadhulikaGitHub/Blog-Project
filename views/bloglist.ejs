<%- include('./header.ejs') %>

    <section class="blog-list mtb-40">
      <div class="container">
        <h1>Blogs</h1>
        <div class="row">
          <% b.forEach((blogItem)=>{ %>
            <div class="col-md-4">
            <div class="blog-box">
              <img src="<%= blogItem.image.url %>" alt="blog1" width="100rem" height="300rem"/>
              <h3><%= blogItem.title %></h3>
              <p>by <span>RSGiTECH</span> on <span><%= blogItem.createdAt.toLocaleDateString() %></span></p>
              <p><%= blogItem.description %></p>
              <a href="/detail/<%= blogItem._id %>" class="btn btn-danger">Read more...</a>
            </div>
          </div>
          <% }) %>
          
